module ModuleBar
{
	def ModuleFunction : (arg : int) -> int
	{
		def arr : int[10] = { 1, 2, 3 }
		for (def i = 0; i < 10; ++i)
		{
			arr[i] += i;
			if (i > 5)
			{
				break;
			}
		}

		def str = "2333"
		unsafe
		{
			def pArrElem = &arr[1];
			*pArrElem += 1;
		}
		return arr[1] + str[2] - '0'
	}
}

def GlobalVariable : int = 5

class Foo
{
	def this : (arg : int)
	{
		Field = arg
	}

	def ~this : ()
	{
	}

	def Method : (arg : int) -> float
	{
		return arg + Field
	}

	def Method2 : () -> void
	{
		def foo : Foo = { 2 }
		foo.Method(foo.Field + Field as int)
	}

	def Field : double = 1.0
}

def Abs : (arg : int) -> int
{
	return arg > 0 ? arg : -arg;
}

def Fib : (arg : int) -> int
{
	if (arg < 2)
		return 1
	else
		return Fib(arg - 2) + Fib(arg - 1)
}

def Func : (arg : int) -> int
{
	if (arg < 0)
	{
		return -arg + GlobalVariable
	}
	else
	{
		return (Interop(arg) as int) * 2
	}
}

def Gcd : (a : int, b : int) -> int
{
	while (b)
	{
		def t = b
		b = a % b
		a = t
	}

	return a;
}

def extern Interop : (arg : float) -> double
