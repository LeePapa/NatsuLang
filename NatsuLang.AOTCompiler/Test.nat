def Abs : (arg : int) -> int
{
	return arg > 0 ? arg : -arg;
}

def Fib : (arg : int) -> int
{
	if (arg < 2)
		return 1
	else
		return Fib(arg - 2) + Fib(arg - 1)
}

def Func : (arg : int) -> int
{
	if (arg < 0)
	{
		return -arg + GlobalVariable
	}
	else
	{
		return (Interop(arg) as int) * 2
	}
}

def Gcd : (a : int, b : int) -> int
{
	while (b)
	{
		def t = b
		b = a % b
		a = t
	}

	return a;
}

def extern Interop : (arg : float) -> double
